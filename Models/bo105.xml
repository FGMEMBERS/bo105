<?xml version="1.0"?>

<PropertyList>
	<path>bo105.ac</path>
	<offsets>
		<!--x-m>2.5</x-m-->
		<z-m>-.2</z-m>
	</offsets>

<!--== shadows ==-->

	<animation>
		<type>texrotate</type>
		<object-name>shadow_rotor</object-name>
		<property>rotors/main/blade1_pos</property>
		<center>
			<x>0.818</x>
			<y>0.168</y>
			<z>0</z>
		</center>
		<axis>
			<z>-1</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>shadow_rotor</object-name>
		<property>orientation/pitch-deg</property>
		<axis>
			<y>-1</y>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>shadow_rotor</object-name>
		<property>orientation/roll-deg</property>
		<axis>
			<x>1</x>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>shadow_rotor</object-name>
		<property>position/altitude-agl-ft</property>
		<factor>0.3</factor>
		<axis>
			<z>-1</z>
		</axis>
	</animation>

	<animation>
		<type>blend</type>
		<object-name>shadow_rotor</object-name>
		<property>rotors/main/rpm</property>
		<factor>0.0045</factor>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>shadow_fuselage</object-name>
		<property>orientation/pitch-deg</property>
		<axis>
			<y>-1</y>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>shadow_fuselage</object-name>
		<property>orientation/roll-deg</property>
		<axis>
			<x>1</x>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>shadow_fuselage</object-name>
		<property>position/altitude-agl-ft</property>
		<factor>0.25</factor>
		<axis>
			<z>-1</z>
		</axis>
	</animation>

	<animation>
		<type>blend</type>
		<object-name>shadow_fuselage</object-name>
		<property>/sim/model/bo105/shadow-blend</property>
	</animation>

