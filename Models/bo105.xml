<?xml version="1.0"?>

<PropertyList>
	<path>bo105.ac</path>
	<offsets>
		<x-m>2.5</x-m>
		<z-m>-.2</z-m>
	</offsets>

	<params>
		<flapA>
			<factor>0.2</factor>
			<max-deg>0.6</max-deg>
		</flapA>
		<flapB>
			<factor>0.3</factor>
			<max-deg>0.6</max-deg>
		</flapB>
		<flapC>
			<factor>0.26</factor>
			<max-deg>0.6</max-deg>
		</flapC>
		<flapD>
			<factor>0.37</factor>
			<max-deg>0.6</max-deg>
		</flapD>
		<flapE>
			<factor>0.4</factor>
			<max-deg>0.6</max-deg>
		</flapE>
	</params>

<!--== shadows ==-->

	<animation>
		<type>noshadow</type>
		<object-name>glass_back_L</object-name>
		<object-name>glass_back_R</object-name>
		<object-name>glass_front_L</object-name>
		<object-name>glass_front_R</object-name>
		<object-name>glass_fixed</object-name>
		<object-name>strobe_halo_Bf</object-name>
		<object-name>strobe_halo_B</object-name>
		<object-name>beacon_halo_B</object-name>
		<object-name>beacon_halo_Bf</object-name>
		<object-name>strobe_halo_T</object-name>
		<object-name>beacon_halo_T</object-name>
	</animation>

	<animation>
		<type>noshadow</type>
		<object-name>emblem</object-name>
	</animation>

	<animation>
		<type>select</type>
		<object-name>shadow_fuselage</object-name>
		<condition>
			<not>
				<property>/sim/rendering/shadows-ac</property>
			</not>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>shadow_rotor</object-name>
		<condition>
			<not>
				<property>/sim/rendering/shadows-ac</property>
			</not>
		</condition>
	</animation>

	<animation>
		<type>texrotate</type>
		<object-name>shadow_rotor</object-name>
		<property>rotors/main/blade1_pos</property>
		<center>
			<x>0.818</x>
			<y>0.168</y>
		</center>
		<axis>
			<z>-1</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>shadow_rotor</object-name>
		<property>orientation/pitch-deg</property>
		<axis>
			<y>-1</y>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>shadow_rotor</object-name>
		<property>orientation/roll-deg</property>
		<axis>
			<x>1</x>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>shadow_rotor</object-name>
		<property>position/altitude-agl-ft</property>
		<factor>0.3</factor>
		<axis>
			<z>-1</z>
		</axis>
	</animation>

	<animation>
		<type>material</type>
		<object-name>shadow_rotor</object-name>
		<transparency>
			<alpha-prop>rotors/main/rpm</alpha-prop>
			<factor>-0.0045</factor>
			<offset>1</offset>
		</transparency>
	</animation>

	<animation>
		<type>rotate</type>
		<name>fu_pitch</name>
		<object-name>shadow_fuselage</object-name>
		<property>orientation/pitch-deg</property>
		<axis>
			<y>-1</y>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<name>fu_roll</name>
		<object-name>shadow_fuselage</object-name>
		<property>orientation/roll-deg</property>
		<axis>
			<x>1</x>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<name>fu_trans</name>
		<object-name>shadow_fuselage</object-name>
		<property>position/altitude-agl-ft</property>
		<factor>0.3</factor>
		<axis>
			<z>-1</z>
		</axis>
	</animation>

	<animation>
		<type>select</type>
		<name>fu_select</name>
		<object-name>shadow_fuselage</object-name>
		<condition>
			<property>sim/model/bo105/shadow</property>
		</condition>
	</animation>

<!--== rotors ==-->

	<!-- tail rotor disc -->

	<animation>
		<type>material</type>
		<object-name>rotor_disc_T</object-name>
		<transparency>
			<alpha-prop>rotors/tail/rpm</alpha-prop>
			<factor>0.0015</factor>
			<offset>-0.2</offset>
			<max>0.55</max>
		</transparency>
		<threshold>0.001</threshold>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>rotor_disc_T</object-name>
		<property>rotors/tail/blade1_pos</property>
		<center>
			<x-m>5.894</x-m>
			<y-m>-0.001</y-m>
			<z-m>1.860</z-m>
		</center>
		<axis>
			<x>-0.032</x>
			<y>0.414</y>
			<z>0.005</z>
		</axis>
	</animation>

	<animation>
		<type>noshadow</type>
		<object-name>rotor_disc_T</object-name>
	</animation>

