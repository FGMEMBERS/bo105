<?xml version="1.0"?>

<PropertyList>
<path>tach.ac</path>

	<animation>
		<type>rotate</type>
		<object-name>Needle</object-name>
		<property>rotors/main/rpm</property>
		<interpolation>
			<entry><ind>  0.0</ind><dep>-135</dep></entry>
			<entry><ind>300.0</ind><dep>   0</dep></entry>
			<entry><ind>600.0</ind><dep> 135</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
		</axis>
	</animation>
</PropertyList>


